generator client {
  provider = "prisma-client-js"
}

datasource db {
   provider = "sqlite" // Change to "postgresql" or "mysql" if using another database
  url      = env("DATABASE_URL")
}

model Card {
  id          Int    @id @default(autoincrement())
  name        String
  designation String
  phone       String @unique
  email       String @unique
}
